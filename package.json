{
  "name": "medic",
  "version": "4.1.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git://github.com/medic/cht-core.git"
  },
  "engines": {
    "node": ">=16.12.0",
    "npm": ">=8.3.1"
  },
  "scripts": {
    "test": "grunt eslint && grunt unit && grunt test-api-integration && grunt e2e",
    "webdriver": "webdriver-manager update && webdriver-manager start",
    "wdio-local": "export VERSION=$(node ./scripts/build/get-version.js) && grunt e2e-env-setup && wdio run ./tests/e2e/default/wdio.conf.js",
    "standard-wdio-local": "export VERSION=$(node ./scripts/build/get-version.js) && grunt e2e-env-setup && wdio run ./tests/e2e/standard/wdio.conf.js",
    "default-wdio-mobile-local": "export VERSION=$(node ./scripts/build/get-version.js) && grunt e2e-env-setup && wdio run ./tests/e2e/default-mobile/wdio.conf.js",
    "e2e-integration": "mocha --config tests/integration/.mocharc.js ",
    "wdio": "wdio run ./tests/e2e/default/wdio.conf.js",
    "upgrade-wdio": "wdio run ./tests/e2e/upgrade/wdio.conf.js",
    "standard-wdio": "wdio run ./tests/e2e/standard/wdio.conf.js",
    "default-wdio-mobile": "wdio run ./tests/e2e/default-mobile/wdio.conf.js",
    "local-images": "export VERSION=$(node ./scripts/build/get-version.js) && grunt build-service-images && grunt create-local-docker-compose-files"
  },
  "devDependencies": {
   
    "@types/chai": "^4.3.4",
    "@types/jquery": "^3.5.6",
    "@types/lodash": "^4.14.175",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.18.34",
    
    "@wdio/allure-reporter": "^8.3.0",
    "@wdio/cli": "^8.3.3",
    "@wdio/devtools-service": "^8.3.3",
    "@wdio/local-runner": "^8.3.3",
    "@wdio/mocha-framework": "^8.3.0",
    "@wdio/spec-reporter": "^8.3.0",
    "allure-commandline": "^2.20.1",
    
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-exclude": "^2.1.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "cht-conf": "^3.15.2",
    "csv-writer": "^1.6.0",
    "deep-equal-in-any-order": "^1.1.15",
    
    "jasmine-spec-reporter": "^7.0.0",
    "jit-grunt": "^0.10.0",
    "jsverify": "^0.8.4",
   
    "lodash": "4.17.19",
    "mocha": "^10.2.0",
    "moment": "^2.29.1",
    "moment-locales-webpack-plugin": "^1.2.0",
    "mustache": "^4.2.0",
    "nodemon": "^2.0.20",
    "raw-loader": "^4.0.2",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
   
  },
  "bundlesize": [
    {
      "path": "./api/build/static/webapp/main.js",
      "maxSize": "950 kB"
    },
    {
      "path": "./api/build/static/webapp/polyfills.js",
      "maxSize": "20 kB"
    },
    {
      "path": "./api/build/static/webapp/runtime.js",
      "maxSize": "1 kB"
    },
    {
      "path": "./api/build/static/webapp/scripts.js",
      "maxSize": "55 kB"
    },
    {
      "path": "./api/build/static/webapp/styles.css",
      "maxSize": "70 kB"
    }
  ]
}
